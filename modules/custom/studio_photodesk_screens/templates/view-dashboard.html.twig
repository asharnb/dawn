{{kint()}}

<div class="row ">
  <div class="col-lg-8">
    <div class="panel panel-default equalheight">
      <div class="panel-heading">
        <div class="panel-title">Production Workflow
        </div>

      </div>
      <div class="panel-body p-xs-imp">


        <div class="col-sm-12 no-margins no-padding ">
          <div class="col-sm-12">
            <div class="col-sm-3">
              <h5 class="m-sm">Total GTINs</h5>
              <h3 class="m-sm">{{content_GTIN.0.count|number_format(0, '.', ',')}}</h3>
            </div>

            <div class="col-sm-3 m-l-n">
              <h5 class="m-sm">Content Complete</h5>
              <h3 class="m-sm">{{content_complete.0.count|number_format(0, '.', ',')}}</h3>
            </div>

            <div class="col-sm-3">
              <h5 class="m-sm">Studio Complete</h5>
              <h3 class="m-sm ">0</h3>
            </div>
            <div class="col-sm-3">
              <h5 class="m-sm">Complete</h5>
              <h3 class="m-sm text-success bold">0</h3>
            </div>
          </div>

        </div>

      </div>
    </div>
  </div>
  <div class="col-lg-4 ">
    <div class="panel panel-default equalheight">

      <div class="panel-heading">
        <div class="panel-title">Launch Goal
        </div>

      </div>

      <div class="panel-body p-xs-imp m-t-n">

        <div class="col-sm-12 no-padding">
          <div class="social-avatar">
            <a href="" class="pull-left">
              <h1>{{'2000000'|number_format(0, '.', ',') }}</h1>
              <small class="text-muted">Under Achieved</small>
            </a>
          </div>
        </div>


      </div>

    </div>
  </div>

</div>


<div class="row ">
  <div class="col-lg-12">
    <div class="panel panel-default equalheight">
      <div class="panel-heading">
        <div class="panel-title">Content Summary
        </div>

      </div>
      <div class="panel-body p-xs-imp">


        <div class="col-sm-12 no-margins no-padding ">
          <div class="col-sm-12">

            <div class="col-sm-3">
              <h5 class="m-sm">Detailer Complete</h5>
              <h3 class="m-sm">{{ content_detailer_complete.0.count|number_format(0, '.', ',')  }}</h3>
            </div>
            <div class="col-sm-3">
              <h5 class="m-sm">Attribute Complete</h5>
              <h3 class="m-sm ">{{ content_attribute_complete.0.count|number_format(0, '.', ',')  }}</h3>
            </div>

            <div class="col-sm-3 m-l-n">
              <h5 class="m-sm">English Complete</h5>
              <h3 class="m-sm">{{ content_english_complete.0.count|number_format(0, '.', ',')  }}</h3>
            </div>

            <div class="col-sm-3">
              <h5 class="m-sm">Arabic Complete</h5>
              <h3 class="m-sm ">{{ content_arabic_complete.0.count|number_format(0, '.', ',')  }}</h3>
            </div>
          </div>

        </div>

      </div>
    </div>
  </div>


</div>

<div class="row ">
  <div class="col-lg-12">
    <div class="panel panel-default equalheight">
      <div class="panel-heading">
        <div class="panel-title">Studio Summary
        </div>

      </div>
      <div class="panel-body p-xs-imp">


        <div class="col-sm-12 no-margins no-padding ">
          <div class="col-sm-12">
            <div class="col-sm-3 m-l-n">
              <h5 class="m-sm">Studio GTINs</h5>
              <h3 class="m-sm">{{studio_GTIN.0.count|number_format(0, '.', ',')}}</h3>
            </div>

            <div class="col-sm-3">
              <h5 class="m-sm">Unmapped GTINs</h5>
              <h3 class="m-sm">{{studio_GTIN_issues.0.count|number_format(0, '.', ',')}}</h3>
            </div>

            <div class="col-sm-3">
              <h5 class="m-sm">Shot/Sourced</h5>
              <h3 class="m-sm">0</h3>
            </div>
            <div class="col-sm-3">
              <h5 class="m-sm">Complete</h5>
              <h3 class="m-sm">{{studio_GTIN_complete.0.count|number_format(0, '.', ',')}}</h3>
            </div>
          </div>

        </div>

      </div>
    </div>
  </div>


</div>

<div class="row ">
  <div class="col-lg-12">
    <div class="panel panel-default equalheight">
      <div class="panel-heading">
        <div class="panel-title">Studio Drill Down
        </div>

      </div>
      <div class="panel-body p-xs-imp">


