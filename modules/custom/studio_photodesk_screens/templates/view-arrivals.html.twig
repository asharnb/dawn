{{kint()}}

<div class="row">
  <div class="row wrapper border-bottom white-bg page-heading">
              <div class="col-lg-8">
                  <h2>Arrivals</h2>
                  <ol class="breadcrumb">
                      <li class="active">
                          <strong>Arrivals</strong>
                      </li>
                  </ol>
              </div>
          </div>
<br>

</div>
<div class="panel panel-default">
  <!-- NEW COL START -->
  <article class="m-sm">
    <table class="table table-responsive table-hover" width="100%">

      <thead>
        <th>Name</th>
        <th>Arriving From</th>
        <th>Time</th>
      </thead>

      <tbody>

        {% for result in results %}


        <tr>
          <td class="do-splitflap">{{ result.name }}</td>
          <td class="do-splitflap">{{ result.arrivalfrom }}</td>
          <td class="do-splitflap">{{ result.time }}</td>
        {% endfor %}
      </tbody>
    </table>

  </article>
  <!-- END COL -->
</div>

<script>
    (function ($) {
        $(document).ready(function () {
            $('.do-splitflap')
                    .splitFlap();
        });
    })(jQuery);
</script>
