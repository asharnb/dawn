
<div class="row">
  <div class="col-sm-12 col-md-8">
    <h3 class="no-margins p-b-sm"><span>Models</span></h3>
    <h4 class="no-margins">View or search all photography models</h4>
  </div>
  <div class="col-sm-12 col-md-4 m-t-md p-r-35">

    <a class="btn btn-primary btn-cons pull-right" href="/node/add/models" >Add New Model</a>
  </div>

</div>
<hr class="simple">

<div class="panel panel-default">
  <div class="">
    <div class="panel-body" style="padding-left:0px !important;">
        <div id="dt_viewmodels_filter" class="col-sm-5 search-container">
              <div class="input-group">
        <span class="input-group-addon" id="basic-addon1"><i class="fa fa-search"></i></span>
        <input type="text" class="form-control inputs" placeholder="Who are you looking for?" id="dt_search_box">


      </div>

        </div>
        <div class="col-sm-4">
      <button type="button" id="btn_search_reset" class="btn">Clear</button>
        </div>

    </div>
  </div>
</div>
<div class="panel panel-default">
  <!-- NEW COL START -->
  <article class="m-sm">
    <table id="dt_viewmodels" class="table table-responsive table-hover" width="100%">

      <thead>
        <th>ID</th>
        <th></th>
        <th>Name</th>
        <th>Gender</th>
        <th>Stats</th>
        <th></th>
      </thead>

      <tbody>






      {% for result in results %}
        <tr>
          <td>{{result.id}}</td>
          <td><img src="{{result.image}}" class="img-circle-table " height="20px"></td>
          <td>{{result.name}}</td>
          <td>{{result.gender == 1 ? 'Male' : 'Female'}}</td>
          <td>{{result.stats}}</td>
          <td>
            <a class="btn btn-xs " href="/node/{{ result.id }}/edit">Edit</a>
          </td>

        </tr>

        {% endfor %}




      </tbody>
    </table>

  </article>
  <!-- END COL -->
</div>
