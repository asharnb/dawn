{#

/**
 * @file
 *
 * Template file for generating the CSS file used for the menu-items
 */

/**
 * Variables:
 * $mlid
 * $path
 */
#}
a.menu-{{ mlid }}, ul.links li.menu-{{ mlid }} a {
  background-image: url({{ path }});
  padding-{{ pos }}:{{ size }}px;
  background-repeat: no-repeat;
  background-position: {{ pos }};
  height: {{ height }}px;
}

